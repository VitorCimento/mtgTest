{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mtgTest": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mtgTest",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              { "input": "src/styles/theme-mono-red.scss"          , "bundleName": "theme-mono-red"          , "inject": false },
              { "input": "src/styles/theme-mono-red-dark.scss"     , "bundleName": "theme-mono-red-dark"     , "inject": false },
              { "input": "src/styles/theme-mono-blue.scss"         , "bundleName": "theme-mono-blue"         , "inject": false },
              { "input": "src/styles/theme-mono-blue-dark.scss"    , "bundleName": "theme-mono-blue-dark"    , "inject": false },
              { "input": "src/styles/theme-mono-green.scss"        , "bundleName": "theme-mono-green"        , "inject": false },
              { "input": "src/styles/theme-mono-green-dark.scss"   , "bundleName": "theme-mono-green-dark"   , "inject": false },
              { "input": "src/styles/theme-mono-white.scss"        , "bundleName": "theme-mono-white"        , "inject": false },
              { "input": "src/styles/theme-mono-white-dark.scss"   , "bundleName": "theme-mono-white-dark"   , "inject": false },
              { "input": "src/styles/theme-mono-black.scss"        , "bundleName": "theme-mono-black"        , "inject": false },
              { "input": "src/styles/theme-mono-black-dark.scss"   , "bundleName": "theme-mono-black-dark"   , "inject": false },
              { "input": "src/styles/theme-golgari-bdom.scss"      , "bundleName": "theme-golgari-bdom"      , "inject": false },
              { "input": "src/styles/theme-golgari-bdom-dark.scss" , "bundleName": "theme-golgari-bdom-dark" , "inject": false },
              { "input": "src/styles/theme-golgari-gdom.scss"      , "bundleName": "theme-golgari-gdom"      , "inject": false },
              { "input": "src/styles/theme-golgari-gdom-dark.scss" , "bundleName": "theme-golgari-gdom-dark" , "inject": false },
              { "input": "src/styles/theme-rakdos-bdom.scss"       , "bundleName": "theme-rakdos-bdom"       , "inject": false },
              { "input": "src/styles/theme-rakdos-bdom-dark.scss"  , "bundleName": "theme-rakdos-bdom-dark"  , "inject": false },
              { "input": "src/styles/theme-rakdos-rdom.scss"       , "bundleName": "theme-rakdos-rdom"       , "inject": false },
              { "input": "src/styles/theme-rakdos-rdom-dark.scss"  , "bundleName": "theme-rakdos-rdom-dark"  , "inject": false },
              { "input": "src/styles/theme-selesnya-gdom.scss"     , "bundleName": "theme-selesnya-gdom"     , "inject": false },
              { "input": "src/styles/theme-selesnya-gdom-dark.scss", "bundleName": "theme-selesnya-gdom-dark", "inject": false },
              { "input": "src/styles/theme-selesnya-wdom.scss"     , "bundleName": "theme-selesnya-wdom"     , "inject": false },
              { "input": "src/styles/theme-selesnya-wdom-dark.scss", "bundleName": "theme-selesnya-wdom-dark", "inject": false },
              { "input": "src/styles/theme-gruul-rdom.scss"        , "bundleName": "theme-gruul-rdom"        , "inject": false },
              { "input": "src/styles/theme-gruul-rdom-dark.scss"   , "bundleName": "theme-gruul-rdom-dark"   , "inject": false },
              { "input": "src/styles/theme-gruul-gdom.scss"        , "bundleName": "theme-gruul-gdom"        , "inject": false },
              { "input": "src/styles/theme-gruul-gdom-dark.scss"   , "bundleName": "theme-gruul-gdom-dark"   , "inject": false },
              { "input": "src/styles/theme-boros-rdom.scss"        , "bundleName": "theme-boros-rdom"        , "inject": false },
              { "input": "src/styles/theme-boros-rdom-dark.scss"   , "bundleName": "theme-boros-rdom-dark"   , "inject": false },
              { "input": "src/styles/theme-boros-wdom.scss"        , "bundleName": "theme-boros-wdom"        , "inject": false },
              { "input": "src/styles/theme-boros-wdom-dark.scss"   , "bundleName": "theme-boros-wdom-dark"   , "inject": false },
              { "input": "src/styles/theme-dimir-udom.scss"        , "bundleName": "theme-dimir-udom"        , "inject": false },
              { "input": "src/styles/theme-dimir-udom-dark.scss"   , "bundleName": "theme-dimir-udom-dark"   , "inject": false },
              { "input": "src/styles/theme-dimir-bdom.scss"        , "bundleName": "theme-dimir-bdom"        , "inject": false },
              { "input": "src/styles/theme-dimir-bdom-dark.scss"   , "bundleName": "theme-dimir-bdom-dark"   , "inject": false },
              { "input": "src/styles/theme-izzet-udom.scss"        , "bundleName": "theme-izzet-udom"        , "inject": false },
              { "input": "src/styles/theme-izzet-udom-dark.scss"   , "bundleName": "theme-izzet-udom-dark"   , "inject": false },
              { "input": "src/styles/theme-izzet-rdom.scss"        , "bundleName": "theme-izzet-rdom"        , "inject": false },
              { "input": "src/styles/theme-izzet-rdom-dark.scss"   , "bundleName": "theme-izzet-rdom-dark"   , "inject": false },
              { "input": "src/styles/theme-orzhov-wdom.scss"       , "bundleName": "theme-orzhov-wdom"       , "inject": false },
              { "input": "src/styles/theme-orzhov-wdom-dark.scss"  , "bundleName": "theme-orzhov-wdom-dark"  , "inject": false },
              { "input": "src/styles/theme-orzhov-bdom.scss"       , "bundleName": "theme-orzhov-bdom"       , "inject": false },
              { "input": "src/styles/theme-orzhov-bdom-dark.scss"  , "bundleName": "theme-orzhov-bdom-dark"  , "inject": false },
              { "input": "src/styles/theme-azorius-wdom.scss"      , "bundleName": "theme-azorius-wdom"      , "inject": false },
              { "input": "src/styles/theme-azorius-wdom-dark.scss" , "bundleName": "theme-azorius-wdom-dark" , "inject": false },
              { "input": "src/styles/theme-azorius-udom.scss"      , "bundleName": "theme-azorius-udom"      , "inject": false },
              { "input": "src/styles/theme-azorius-udom-dark.scss" , "bundleName": "theme-azorius-udom-dark" , "inject": false }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mtgTest:build",
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "mtgTest:build:production",
              "proxyConfig": "src/proxy.conf.json"
            },
            "development": {
              "browserTarget": "mtgTest:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mtgTest:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "mtgTest"
}
